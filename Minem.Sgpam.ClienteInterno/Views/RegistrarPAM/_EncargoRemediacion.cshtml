@model IEnumerable<Minem.Sgpam.TransporteDatos.DtoEntidades.Comp_Enc_RemDTO>

<div class="x_titleGrupo">
    <div align="center" class="titulo-m">
    </div>

    <ul class="nav navbar-right panel_toolbox">
        <li>
            <div class="boton">
                <a href="#" data-toggle="modal" data-target="#modal20" onclick="ShowModal('@Url.Action("CrearEncargoRemediacion", "RegistrarPAM")', 'fullEncargoRemediacion')">Agregar  <i class="fa fa-plus"></i></a>
            </div>
        </li>
    </ul>
    <div class="clearfix"></div>
</div>

<div class="clearfix"></div>
<div class="table-responsive">
    <table id="fulltbl14" class="table table-striped jambo_table bulk_action">
        <thead>
            <tr class="headings">
                <th class="column-title">NRO</th>
                <th class="column-title"> Resolución de Encargo</th>
                <th class="column-title"> Responsable de la Remediación</th>
                <th class="column-title">Resolución de Exclusión del Encargo de Remediación</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr class="odd pointer">
                    <td class=" ">
                        @item.Nro_Remediacion
                    </td>
                    <td class=" ">
                        @item.Resolucion_Exc_Enc
                    </td>
                    <td class=" ">
                        @item.Responsable_Remediacion
                    </td>
                    <td class=" ">
                        @item.Resolucion_Exc_Enc
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>