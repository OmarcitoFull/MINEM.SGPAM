        function MostrarReconocimiento(vIdTipoMineria) {
            switch (vIdTipoMineria) {
                case "@((int)TipoMineria.MINERIA_SUBTERRANEA)":
                    $("#subterranea").show();
                    $("#superficial").hide();
                    break;
                case "@((int)TipoMineria.MINERIA_SUPERFICIAL)":
                    $("#subterranea").hide();
                    $("#superficial").show();
                    break;
                default:
                    $("#subterranea").hide();
                    $("#superficial").hide();
                    $("#vId_Tipo_Mineria").val("");
            }
        }

