                    <tr class="headings">
                        @*<th class="column-title">Componente</th>*@
                        <th class="column-title">Codigo </th>
                        <th class="column-title">Nombre </th>
                        <th class="column-title">UEA</th>
                        <th class="column-title">Tipo</th>
                        <th class="column-title">Sustancia</th>
                        <th class="column-title">Situación</th>
                        <th class="column-title">Estado</th>
                        @*<th class="column-title">Resolución que otrorga</th>
                            <th class="column-title">Fecha RO</th>
                            <th class="column-title">Resolución de caducidad</th>
                            <th class="column-title">Fecha RCv</th>*@
                    </tr>
                </thead>
                <tbody>
                    @if (!Model.ListaDerechosMineros.Any())
                    {
                        <tr class="even pointer">
                            <td class=" " colspan="7">No existen registros</td>
                        </tr>
                    }
                    else
                    {
                        foreach (var item in Model.ListaDerechosMineros)
                        {
                            <tr class="even pointer">
                                @*<td class=" ">@item.Nombre_Comp</td>*@
                                <td class=" ">@item.Codigo_Dm</td>
                                <td class=" ">@item.Descripcion_Dm</td>
                                <td class=" ">@item.Uea</td>
                                <td class=" ">@item.Des_Tipo</td>
                                <td class=" ">@item.Des_Sustancia</td>
                                <td class=" ">@item.Des_Situacion</td>
                                <td class=" ">@item.Des_Estado</td>
                                @*<td class=" "></td>
                                    <td class=" "></td>
                                    <td class=" "></td>
                                    <td class=" "></td>*@
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>

        <div class="clearfix"></div>
        <div class="clearfix"></div>
    </div>


    <div class="rowGroup">
        <h1 class="grupoT">10. Situación y Principios Productos</h1>
        <div class="table-responsive">
            <table class="table table-striped jambo_table bulk_action">
                <thead>
                    <tr class="headings">
                        <th class="column-title">Año </th>
                        <th class="column-title">Nombre Derecho Minero o UEA </th>
                        <th class="column-title">Situación</th>
                        <th class="column-title">Tipo Concentrado</th>
                        <th class="column-title">Cantidad (TM)</th>
                    </tr>
                </thead>
                <tbody>
                    @if (!Model.ListaSituacionPrincipalesProducto.Any())
                    {
                        <tr class="even pointer">
                            <td class=" " colspan="5">No existen registros</td>
                        </tr>
                    }
                    else
                    {
                        foreach (var item in Model.ListaSituacionPrincipalesProducto)
                        {
                            <tr class="even pointer">
                                <td class=" col-lg-1 ">@item.Anio</td>
                                <td class=" col-lg-5 ">@item.Nombre_Dm</td>
                                <td class=" col-lg-3 ">@item.Situacion_Up</td>
                                <td class=" col-lg-2 ">@item.Tipo_Concentrado</td>
                                <td class=" col-lg-1 ">@item.Cantidad</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>

        <div class="clearfix"></div>
        <div class="clearfix"></div>
    </div>


    <div class="rowGroup">
        <h1 class="grupoT">11. Titulares Referenciales</h1>
        <div class="table-responsive">
            <table class="table table-striped jambo_table bulk_action">
                <thead>
                    <tr class="headings">
                        <th class="column-title">Titular </th>
                        <th class="column-title">Nombre Derecho Minero </th>
                        <th class="column-title">Fecha Inicio</th>
                        <th class="column-title">Fecha Fin</th>
                        <th class="column-title">Estado</th>

                    </tr>
                </thead>
