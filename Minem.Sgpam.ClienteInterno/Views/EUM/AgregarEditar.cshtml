@using Minem.Sgpam.InfraEstructura;
@using Minem.Sgpam.InfraEstructura.Enumerados;
@model Minem.Sgpam.TransporteDatos.DtoEntidades.RegistrarEumDTO

@{
    ViewData["Title"] = "Ex Unidad Minera";
}

<form id="MyForm" asp-controller="Eum" asp-action="AgregarEditar" autocomplete="off" data-ajax="true" data-ajax-method="POST">

    <div class="rowGroup">
        <h1 class="grupoT">1. Historial Modificaciones</h1>
        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Nombre EUM (<font color="red">*</font>)</label>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <input asp-for="@Model.Eum.Eum_Descripcion" class="form-control text-uppercase" maxlength="100" />
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Acceso a EUM (<font color="red">*</font>)</label>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <input asp-for="@Model.Eum.Acceso_Eum" class="form-control" maxlength="300" />
                </div>
            </div>
        </div>

        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
            <table class="table table-striped jambo_table bulk_action">
                <thead>
                    <tr class="headings">
                        <th class="column-title">Usuario </th>
                        <th class="column-title">Cargo </th>
                        <th class="column-title">Fecha de Modificación</th>
                    </tr>
                </thead>
                <tbody>
                    @if (!Model.ListaHistorial.Any())
                    {
                        <tr class="even pointer">
                            <td class=" " colspan="3">No existen registros</td>
                        </tr>
                    }
                    else
                    {
                        @foreach (var item in Model.ListaHistorial)
                        {
                            <tr class="even pointer">
                                <td class=" ">@item.Usu_Ingreso</td>
                                <td class=" ">@item.Cargo</td>
                                <td class=" ">@item.Fec_Ingreso</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
        <div class="clearfix"></div>
    </div>

    <div class="rowGroup">
        <h1 class="grupoT">2. Inspecciòn</h1>
        <div class="x_titleGroup">

            <div align="center" class="titulo-m">
            </div>
            <ul class="nav navbar-right panel_toolbox">
                <li>
                    @if (@Model.Eum.Id_Eum != 0)
                    {
                        <div class="boton">
                            <a href="#" data-toggle="modal" data-target="#modal2" onclick="ShowModal('@Url.Action("CrearInspeccion", "Eum")', 'fullInspeccion')">Agregar  <i class="fa fa-plus"></i></a>
                        </div>
                    }
                </li>
            </ul>
            <div class="clearfix"></div>
        </div>

        <div class="table-responsive">
            <table class="table table-striped jambo_table bulk_action">
                <thead>
                    <tr class="headings">
                        <th class="column-title">Nombre de Inspector </th>
                        <th class="column-title">Fecha de Inspeccion </th>
                        <th class="column-title">Hora Inspección</th>
                        <th class="column-title">Descripción del Clima</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model.ListaInspeccion == null)
                    {
                        <tr class="even pointer">
                            <td class=" " colspan="4">No existen registros</td>
                        </tr>
                    }
                    else
                    {
                        foreach (var item in Model.ListaInspeccion)
                        {
                            <tr class="even pointer">
                                <td class=" col-lg-3 ">@item.Nomnbre_Inspector </td>
                                <td class=" col-lg-1 ">@item.Fecha_Inspeccion.ToShortDateString()</td>
                                <td class=" col-lg-1 ">@item.Fecha_Inspeccion.ToShortTimeString()</td>
                                <td class=" col-lg-4 ">@item.Descripcion_Clima</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
        <div class="clearfix"></div>
    </div>


    <div class="rowGroup">
        <h1 class="grupoT">3. Ubicación</h1>
        <div class="table-responsive">
            <table class="table table-striped jambo_table bulk_action">
                <thead>
                    <tr class="headings">
                        <th class="column-title">Region </th>
                        <th class="column-title">Provincia </th>
                        <th class="column-title">Distrito</th>
                        <th class="column-title">Uni. Hidrográfica</th>
                        <th class="column-title">Cuenca Principal</th>
                    </tr>
                </thead>
                <tbody>
                    @if (!Model.ListaUbicacion.Any())
                    {
                        <tr class="even pointer">
                            <td class=" " colspan="5">No existen registros</td>
                        </tr>
                    }
                    else
                    {
                        foreach (var item in Model.ListaUbicacion)
                        {
                            <tr class="even pointer">
                                <td class=" ">@item.Departamento</td>
                                <td class=" ">@item.Provincia</td>
                                <td class=" ">@item.Distrito</td>
                                <td class=" ">@item.Uni_Hidrografica</td>
                                <td class=" ">@item.Cuenca_Principal</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
        <div class="clearfix"></div>
    </div>


    <div class="rowGroup">
        <h1 class="grupoT">4. Información adicional</h1>
        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Historia EUM (<font color="red">*</font>)</label>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <textarea asp-for="@Model.Eum.Historia_Eum" rows="3" class="form-controlArea" maxlength="400"></textarea>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Evidencia de actividades recientes (<font color="red">*</font>)</label>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <textarea asp-for="@Model.Eum.Evidencia_Act_Rec" rows="3" class="form-controlArea  has-feedback-left" maxlength="400"></textarea>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
    </div>


    <div class="rowGroup">
        <h1 class="grupoT">5. Tipo de operación y sustancia</h1>
        @*<div class="col-md-6 col-sm-12 col-xs-12 form-group">
                <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Tipo operación:</label>
                    <div class="col-md-9 col-sm-9 col-xs-12">
                        <select id="vId_Tipo_Operacion" class="select2_single form-control" asp-for="@Model.Eum.Id_Tipo_Operacion" asp-items="(List<SelectListItem>)ViewBag.CboTipoOperacion">
                            <option value="">Seleccione</option>
                        </select>
                        <input type="hidden" asp-for="@Model.Eum.Id_Tipo_Operacion" />
                    </div>
                </div>
            </div>*@
        <div class="col-md-5 col-sm-12 col-xs-12 form-group">
            <table class="table table-striped jambo_table bulk_action">
                <thead>
                    <tr class="headings">
                        <th class="column-title">ID </th>
                        <th class="column-title">Tipo Operación</th>
                        <th class="column-title "></th>
                    </tr>
                </thead>
                <tbody>
                    @if (!Model.ListaOperacion.Any())
                    {
                        <tr class="even pointer">
                            <td class=" " colspan="3">No existen registros</td>
                        </tr>
                    }
                    else
                    {
                        @foreach (var item in Model.ListaOperacion)
                        {
                            <tr class="even pointer">
                                <td class=" ">@item.Id_Eum_Operacion</td>
                                <td class=" ">@item.Tipo_Operacion</td>
                                <td style="width:60px">
                                    <ul class="nav navbar-center panel_toolbox">
                                        <li>
                                            <div class="boton2">
                                                @*<a asp-action="EliminarEumOperacion" asp-route-vId="@item.Id_Eum_Operacion" onClick="Confirm('Seguro de eliminar')" title="Eliminar Registro"><i class="fa fa-trash-o "></i></a>*@
                                                <a href="#" onclick="RemoveEumOperacion(@item.Id_Eum_Operacion)" title="Eliminar Registro"> <i class="fa fa-trash-o "></i></a>
                                            </div>
                                        </li>
                                    </ul>
                                </td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
        <div class="col-md-3 col-sm-12 col-xs-12 form-group">
            <div class="x_titleGroup">

                <div align="center" class="titulo-m">
                </div>
                <ul class="nav navbar-left panel_toolbox">
                    <li>
                        @if (@Model.Eum.Id_Eum != 0)
                        {
                            <div class="boton">
                                <a href="#" data-toggle="modal" data-target="#modal5" onclick="ShowModal('@Url.Action("CrearTipoOperacion", "Eum")', 'fullTipoOperacion')">Add. Tipo Operación  <i class="fa fa-plus"></i></a>
                            </div>
                        }
                    </li>
                </ul>
            </div>
        </div>

        <div class="col-md-4 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-4 col-sm-4 col-xs-12">Tipo sustancia (<font color="red">*</font>)</label>
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <select id="vId_Tipo_Sustancia" class="select2_single form-control" asp-for="@Model.Eum.Id_Tipo_Sustancia" asp-items="(List<SelectListItem>)ViewBag.CboTipoSustancia">
                        <option value="">Seleccione</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="col-md-4 col-sm-12 col-xs-12 form-group">
            <div class="form-group" style="visibility:hidden">
                <label class="control-label col-md-4 col-sm-4 col-xs-12">ID EUM:</label>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <input asp-for="@Model.Eum.Id_Eum" class="form-control" readonly="readonly" />
                </div>
            </div>
        </div>

        <div class="clearfix"></div>
    </div>


    <div class="rowGroup">
        <h1 class="grupoT">6. Ambiente Circulante</h1>
        <div class="col-md-4 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-4 col-sm-4 col-xs-12">Relieve (<font color="red">*</font>)</label>
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <input asp-for="@Model.Eum.Relieve" class="form-control" maxlength="300" />
                </div>
            </div>
        </div>
        <div class="col-md-4 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-4 col-sm-4 col-xs-12">Cuerpos de agua (<font color="red">*</font>)</label>
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <input asp-for="@Model.Eum.Cuerpos_Agua" class="form-control" maxlength="100" />
                </div>
            </div>
        </div>
        <div class="col-md-4 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-4 col-sm-4 col-xs-12">Conflicto social (<font color="red">*</font>)</label>
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <select id="vId_Conflicto_Social" class="select2_single form-control" asp-for="@Model.Eum.Id_Conflicto_Social" asp-items="(List<SelectListItem>)ViewBag.CboConflictoSocial">
                        <option value="">Seleccione</option>
                    </select>
                </div>
            </div>
        </div>

        <div class="col-md-4 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-4 col-sm-4 col-xs-12">Flora Terr. (<font color="red">*</font>)</label>
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <input asp-for="@Model.Eum.Flora_Terrestre" class="form-control" maxlength="300" />
                </div>
            </div>
        </div>
        <div class="col-md-4 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-4 col-sm-4 col-xs-12">Fauna Terr. (<font color="red">*</font>)</label>
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <input asp-for="@Model.Eum.Fauna_Terreste" class="form-control" maxlength="300" />
                </div>
            </div>
        </div>
        <div class="col-md-4 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-4 col-sm-4 col-xs-12">Flora y Fauna Acuat. (<font color="red">*</font>)</label>
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <input asp-for="@Model.Eum.Flora_Fauna_Acuatica" class="form-control" maxlength="300" />
                </div>
            </div>
        </div>

        <div class="col-md-4 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-4 col-sm-4 col-xs-12">Infraestructura urbana (<font color="red">*</font>)</label>
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <input asp-for="@Model.Eum.Infra_Urbana" class="form-control" maxlength="300" />
                </div>
            </div>
        </div>
        <div class="col-md-4 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-4 col-sm-4 col-xs-12">Usos de Suelo (<font color="red">*</font>)</label>
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <input asp-for="@Model.Eum.Uso_Suelo" class="form-control" maxlength="100" />
                </div>
            </div>
        </div>
        <div class="col-md-4 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-4 col-sm-4 col-xs-12">Usos de Agua (<font color="red">*</font>)</label>
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <input asp-for="@Model.Eum.Uso_Agua" class="form-control" maxlength="300" />
                </div>
            </div>
        </div>

        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Áreas de conservación (<font color="red">*</font>)</label>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <input asp-for="@Model.Eum.Area_Conservacion" class="form-control" maxlength="300" />
                </div>
            </div>
        </div>

        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Sitios Arq. o Turist. (<font color="red">*</font>)</label>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <input asp-for="@Model.Eum.Sitio_Arque_Turistico" class="form-control" maxlength="300" />
                </div>
            </div>
        </div>

        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Conflicto Social</label>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <input asp-for="@Model.Eum.Conflicto_Social" class="form-control" maxlength="300" />
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
    </div>


    <div class="rowGroup">
        <h1 class="grupoT">7. Comentarios adicional</h1>
        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Descripción EUM (<font color="red">*</font>)</label>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <textarea asp-for="@Model.Eum.Descripcion_Eum" class="form-controlArea  has-feedback-left" rows="2" maxlength="400"></textarea>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
            <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Comentarios adicional</label>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <textarea asp-for="@Model.Eum.Comentario_Adi" class="form-controlArea  has-feedback-left" rows="4" placeholder="" maxlength="450"></textarea>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
    </div>


    <div class="rowGroup">
        <h1 class="grupoT">8. Información Gráfica</h1>
        <div class="x_titleGroup">

            <div align="center" class="titulo-m">
            </div>
            <ul class="nav navbar-right panel_toolbox">
                <li>
                    @if (@Model.Eum.Id_Eum != 0)
                    {
                        <div class="boton">
                            <a href="#" data-toggle="modal" data-target="#modal8" onclick="ShowModal('@Url.Action("CrearInfoGrafica", "Eum")', 'fullInfoGrafica')">Agregar  <i class="fa fa-plus"></i></a>
                        </div>
                    }
                </li>
            </ul>
            <div class="clearfix"></div>
        </div>

        <div class="table-responsive">
            <table class="table table-striped jambo_table bulk_action">
                <thead>
                    <tr class="headings">
                        <th class="column-title">Nombre de fotografia </th>
                        <th class="column-title">Fecha de toma </th>
                        <th class="column-title">Tamaño </th>
                        <th class="column-title">Descripción </th>
                        <th class="column-title no-link last" style="text-align:center"><span class="nobr">Opciones</span></th>
                    </tr>
                </thead>
                <tbody>
                    @if (!Model.ListaInformacionGrafica.Any())
                    {
                        <tr class="even pointer">
                            <td class=" " colspan="5">No existen registros</td>
                        </tr>
                    }
                    else
                    {
                        foreach (var item in Model.ListaInformacionGrafica)
                        {
                            <tr class="even pointer">
                                <td class=" col-lg-2 ">@item.Nombre_Imagen</td>
                                <td class=" col-lg-1 ">@item.Fecha_Toma</td>
                                <td class=" col-lg-1 ">@item.Tamano</td>
                                <td class=" col-lg-4 ">@item.Descripcion</td>
                                <td class=" col-lg-1" style="text-align:center">
                                    @if (item.Id_LaserFiche.GetValueOrDefault() > 0)
                                    {
                                        <button type="button" class="boton2" style="margin-right: 0px;" onclick="DescargarArchivo(@item.Id_LaserFiche.ToString(), '@item.Nombre_Imagen')"> <i class="fa fa-download"></i></button>
                                    }
                                </td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>

    </div>


    <div class="rowGroup">
        <h1 class="grupoT">9. Derechos Mineros</h1>
        <div class="table-responsive">
            <table class="table table-striped jambo_table bulk_action">
