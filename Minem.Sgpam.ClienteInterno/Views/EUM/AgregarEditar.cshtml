@section Scripts {
    <script type="text/javascript">
        function FullNewComponent() {
            $.post("@Url.Action("CreateComponent", "Eum")?vIdEum=" + @Model.Eum.Id_Eum + "&vIdTipo=" + $("#cboTipoComponente").val()).done(function (x)
            {
                if (x.operation == "@Constantes.Ok") {
                    alert(x.key);
                    switch ($("#cboTipoComponente").val()) {
                        case "@((int)TipoPam.LABOR_MINERA)":
                            window.location.href = "@Url.Action("LaborMinera", "RegistrarPAM")?vId=" + x.key;
                            break;
                        case "@((int)TipoPam.RESIDUO_MINERO)":
                            window.location.href = "@Url.Action("ResiduoMinero", "RegistrarPAM")?vId=" + x.key;
                            break;
                        case "@((int)TipoPam.OTRO_RESIDUO)":
                            window.location.href = "@Url.Action("OtrosResiduos", "RegistrarPAM")?vId=" + x.key;
                            break;
                        case "@((int)TipoPam.INFRAESTRUCTURA)":
                            window.location.href = "@Url.Action("Infraestructura", "RegistrarPAM")?vId=" + x.key;
                            break;
                        case "@((int)TipoPam.SUSTANCIA_QUIMICA)":
                            window.location.href = "@Url.Action("SustanciasQuimicas", "RegistrarPAM")?vId=" + x.key;
                            break;
                    }
                    //window.location.reload();
                } else {
                    alert("Sistema : Ocurrió un error al grabar en el servidor")
                }
            }).fail(function (a, b, c) {
                alert("Result: " + b + " " + c + " " + a.status + " " + a.statusText)
            });
        }

        function FullRemoveComponent(vId) {
            $.post("@Url.Action("RemoveComponent", "Eum")?vId=" + vId).done(function (x)
            {
                if (x.operation == "@Constantes.Ok") {
                    Success("@Constantes.MsgOk");
                    window.location.reload();
                } else {
                    Error("@Constantes.MsgErr");
                }
            }).fail(function (a, b, c) {
                alert("Result: " + b + " " + c + " " + a.status + " " + a.statusText)
            });
        }

        $('#btnSubmit').click(function () {
            var vModel = $("#MyForm").serializeArray();
            //console.log(vModel);

            var vData = {};
            $.map(vModel, function (n, i) { vData[n.name.replace("Eum.", "")] = n.value; });
            //console.log(vData);

            $.ajax({
                type: "POST",
                url: $("#MyForm").attr("action"),
                data: vData,
                success: function (data) {
                    if (data.operation == "@Constantes.Ok") {
                        Success("@Constantes.MsgOk");
                        window.location.reload();
                    } else {
                        //Error("@Constantes.MsgErr");
                        "@((int)TipoErr.MODEL)" == data.type ? Info("@Constantes.MsgModel") : Error("@Constantes.MsgErr")
                    }
                },
                error: function (req, status, error) {
                    //console.log(req)
                    alert(req);
                    alert(status);
                    alert(error);
                }
            });


        });


        $("#btnReporte").click(function () {
            window.location = "@Url.Action("DescargarInformePam", "Eum")?vKey=1";
        });


        ShowModal = function (vUrl, vBody) {
            $.get(vUrl + "?vIdEum=@Model.Eum.Id_Eum", function (data) { $('#'+vBody).html(data) });
