@model Minem.Sgpam.TransporteDatos.DtoEntidades.Eum_Info_GraficaDTO

<div class="rowGroup">
    <h1 class="grupoT">Agregar información gráfica</h1>

    <div class="col-md-12 col-sm-12 col-xs-12 form-group">
        <div class="form-group">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">Fecha:</label>
            <div class="col-md-9 col-sm-9 col-xs-12">
                <input type="date" class="form-control has-feedback-left" id="single_cal2" placeholder="First Name" aria-describedby="inputSuccess2Status2">
            </div>
            <input type="hidden" asp-for="Fec_Ingreso" />
        </div>
    </div>

    <div class="col-md-12 col-sm-12 col-xs-12 form-group">
        <div class="form-group">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">Descripción</label>
            <div class="col-md-9 col-sm-9 col-xs-12">
                <textarea class="form-controlArea " rows="3" placeholder=""></textarea>
            </div>
            <input type="hidden" asp-for="Nombre_Imagen" />
        </div>
    </div>

    <div class="col-md-12 col-sm-12 col-xs-12 form-group">
        <div class="form-group">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">Imagen</label>
            <div class="col-md-9 col-sm-9 col-xs-12">
                <input type="file" class="form-control-file" id="exampleFormControlFile1">
            </div>
        </div>
    </div>

    <div class="clearfix"></div>

    <div class="x_title2">

        <div align="center" class="titulo-m">
            <input type="hidden" asp-for="Id_Eum" />
        </div>

        <ul class="nav navbar-right panel_toolbox">
            <li>
                <div class="boton"><a href="#">Guardar  <i class="fa fa-save"></i></a></div>
                <button type="button" class="boton" style="margin-right: 0px;" id="GuardarInspeccion" onclick="SaveInspeccion()">Guardar  <i class="fa fa-save" style="margin-left: 10px;"></i></button>
            </li>
            <li>
                <div class="boton" data-dismiss="modal">
                    <a href="#">Cancelar  <i class="fa fa-ban"></i></a>
                </div>
            </li>
        </ul>

        <div class="clearfix"></div>
    </div>

    <div class="clearfix"></div>
</div>